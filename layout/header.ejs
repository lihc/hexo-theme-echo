<header>
    <div>
        <a href="/"><%= config.title  %> </a>
    </div>
    <nav>
        <%- search_form() %>
        <%_ for (const menu in theme.menus) { -%>
        <a class="<%- is_current(theme.menus[menu]) ? 'active' : '' %>" href="<%- url_for(theme.menus[menu]) %>"><%= __(menu) %></a>
        <%_ } -%>
    </nav>
</header>